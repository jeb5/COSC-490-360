{
	"configurations": [
		{
			"name": "Python Debug Angle Estimation",
			"type": "debugpy",
			"request": "launch",
			"program": "src/angle_estimation.py",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}",
			"args": [
				"input/balloon_footage.MP4",
				"input/gyro_data.csv",
				"temp/balloon_test.csv",
				"--output_video",
				"output/balloon_test.mp4",
				"--start_frame",
				"68000",
				"--end_frame",
				"70000",
				"--gopro",
				"--output_debug_video",
				"output/balloon_test_debug.mp4"
			]
		},
		{
			"name": "Python Debug 360 Processing",
			"type": "debugpy",
			"request": "launch",
			"program": "src/360.py",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}",
			"args": ["output/balloon_test.mp4", "temp/balloon_test.csv", "output/balloon_test_mapped.mp4", "--scale", "0.25"]
		}
	]
}
